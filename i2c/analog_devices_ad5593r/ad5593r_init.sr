[global]
sigrok version=0.5.2

[device 1]
driver=demo
samplerate=1000000
unitsize=1
total probes=2
probe1=SCL
probe2=SDA

[data]
# AD5593R initialization sequence
# I2C address: 0x10 (ADDR pins = 000)
# Sample rate: 1 MHz
# Operations:
# 1. Power reference control (enable internal reference)
# 2. General purpose configuration
# 3. ADC pin configuration (channels 0-3 as ADC)
# 4. DAC pin configuration (channels 4-5 as DAC)
# 5. GPIO configuration (channels 6-7 as GPIO)
